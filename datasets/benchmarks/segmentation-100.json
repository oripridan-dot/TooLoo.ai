  { "id": "case_048", "description": "Long: 7 topics, rapid switches", "input": { "text": "User: T1\nAssistant: Ok\nUser: T2\nAssistant: Ok\nUser: T3\nAssistant: Ok\nUser: T4\nAssistant: Ok\nUser: T5\nAssistant: Ok\nUser: T6\nAssistant: Ok\nUser: T7\nAssistant: Ok" }, "expected": { "type": "segments", "segments": 7, "tolerance": 2 }, "endpoint": "/api/v1/analyze-text" },
