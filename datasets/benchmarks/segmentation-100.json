  { "id": "case_050", "description": "Edge: only user, long reply", "input": { "text": "User: Here is a long message about many things." }, "expected": { "type": "segments", "segments": 1, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_051", "description": "Multi-user: user, admin, assistant, system, 5 topics", "input": { "text": "User: T1\nAdmin: T2\nAssistant: T3\nSystem: T4\nUser: T5" }, "expected": { "type": "segments", "segments": 5, "tolerance": 2 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_052", "description": "Ambiguous: topic switch with time", "input": { "text": "User: Let's talk about A. Later, B." }, "expected": { "type": "segments", "segments": 2, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
