{
  "name": "segmentation-100",
  "description": "Comprehensive 100-case segmentation benchmark for TooLoo.ai",
  "version": "1.0",
  "cases": [
    {
      "id": "case_001",
      "description": "Basic user-assistant Q&A",
      "input": { "text": "User: What's the weather today?\nAssistant: It's sunny and 75 degrees.\nUser: Should I bring an umbrella?\nAssistant: No need, no rain expected." },
      "expected": { "type": "segments", "segments": 2, "tolerance": 1 },
      "endpoint": "/api/v1/analyze-text"
    },
    {
      "id": "case_002",
      "description": "Rapid topic switch",
      "input": { "text": "User: How do I reset my password?\nAssistant: Go to settings and click 'Reset Password'.\nUser: Also, what's the capital of France?\nAssistant: Paris is the capital of France." },
      "expected": { "type": "segments", "segments": 2, "tolerance": 1 },
      "endpoint": "/api/v1/analyze-text"
    },
    {
      "id": "case_003",
      "description": "Long technical troubleshooting",
      "input": { "text": "User: My app keeps crashing.\nAssistant: What error message do you see?\nUser: It says 'NullReferenceException'.\nAssistant: That usually means you're trying to access something that doesn't exist.\nUser: How do I fix it?\nAssistant: Check for null before accessing objects.\nUser: Thanks!\nAssistant: You're welcome." },
      "expected": { "type": "segments", "segments": 2, "tolerance": 1 },
      "endpoint": "/api/v1/analyze-text"
    },
    {
      "id": "case_004",
      "description": "Mixed business and personal topics",
      "input": { "text": "User: Let's review the quarterly report.\nAssistant: Sure, the numbers look good.\nUser: By the way, did you watch the game last night?\nAssistant: Yes, it was exciting!\nUser: Back to the report, any concerns?\nAssistant: Just a few minor issues." },
      "expected": { "type": "segments", "segments": 3, "tolerance": 1 },
      "endpoint": "/api/v1/analyze-text"
    },
    {
      "id": "case_005",
      "description": "Ambiguous topic boundaries",
      "input": { "text": "User: I need help with my taxes.\nAssistant: What kind of help do you need?\nUser: Filing and deductions.\nAssistant: I can assist with both.\nUser: Also, my car won't start.\nAssistant: Let's troubleshoot that." },
      "expected": { "type": "segments", "segments": 2, "tolerance": 1 },
      "endpoint": "/api/v1/analyze-text"
    },
  { "id": "case_006", "description": "Multi-user chat", "input": { "text": "User: Can you help?\nUser2: I have the same issue.\nAssistant: Let's solve it together." }, "expected": { "type": "segments", "segments": 2, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_007", "description": "Short single-topic", "input": { "text": "User: Hi\nAssistant: Hello!" }, "expected": { "type": "segments", "segments": 1, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_008", "description": "Long mixed-topic", "input": { "text": "User: Let's discuss marketing.\nAssistant: Sure.\nUser: Now, about engineering.\nAssistant: Switching topics.\nUser: Back to marketing.\nAssistant: Ok." }, "expected": { "type": "segments", "segments": 3, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_009", "description": "Rapid-fire Q&A", "input": { "text": "User: Color of sky?\nAssistant: Blue.\nUser: Capital of Spain?\nAssistant: Madrid.\nUser: 2+2?\nAssistant: 4." }, "expected": { "type": "segments", "segments": 3, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_010", "description": "Nested topics", "input": { "text": "User: Let's talk about travel.\nAssistant: Where to?\nUser: Paris.\nAssistant: Great choice.\nUser: Also, food in Paris.\nAssistant: Lots of options." }, "expected": { "type": "segments", "segments": 2, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_011", "description": "Ambiguous switch", "input": { "text": "User: I need help.\nAssistant: With what?\nUser: My phone.\nAssistant: What model?\nUser: Actually, my laptop.\nAssistant: Ok, let's switch." }, "expected": { "type": "segments", "segments": 2, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_012", "description": "Long single-topic", "input": { "text": "User: Let's plan a party.\nAssistant: Sure.\nUser: Who to invite?\nAssistant: Friends and family.\nUser: What food?\nAssistant: Pizza and salad.\nUser: What games?\nAssistant: Board games." }, "expected": { "type": "segments", "segments": 1, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_013", "description": "Short ambiguous", "input": { "text": "User: Help!\nAssistant: What's wrong?\nUser: Everything.\nAssistant: Let's start with one thing." }, "expected": { "type": "segments", "segments": 1, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_014", "description": "Multi-turn technical", "input": { "text": "User: How to fix error 404?\nAssistant: Check the URL.\nUser: Still not working.\nAssistant: Try clearing cache." }, "expected": { "type": "segments", "segments": 1, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_015", "description": "Multi-user rapid switch", "input": { "text": "User: What's the time?\nUser2: How do I login?\nAssistant: It's 2pm.\nAssistant: Use your email to login." }, "expected": { "type": "segments", "segments": 2, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_016", "description": "Edge: empty input", "input": { "text": "" }, "expected": { "type": "segments", "segments": 0, "tolerance": 0 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_017", "description": "Edge: only assistant", "input": { "text": "Assistant: Hello!" }, "expected": { "type": "segments", "segments": 1, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_018", "description": "Edge: only user", "input": { "text": "User: Hi there!" }, "expected": { "type": "segments", "segments": 1, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_019", "description": "Long multi-topic", "input": { "text": "User: Let's talk about health.\nAssistant: Sure.\nUser: Now, about finance.\nAssistant: Ok.\nUser: Back to health.\nAssistant: Got it." }, "expected": { "type": "segments", "segments": 3, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_020", "description": "Short multi-topic", "input": { "text": "User: Weather?\nAssistant: Sunny.\nUser: News?\nAssistant: Stocks up." }, "expected": { "type": "segments", "segments": 2, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_021", "description": "Rapid ambiguous switch", "input": { "text": "User: Help with login.\nAssistant: Use your email.\nUser: Also, my printer.\nAssistant: Try restarting." }, "expected": { "type": "segments", "segments": 2, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_022", "description": "Multi-user, multi-topic", "input": { "text": "User: How do I pay?\nUser2: How do I cancel?\nAssistant: Pay via card.\nAssistant: Cancel in settings." }, "expected": { "type": "segments", "segments": 2, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
     { "id": "case_023", "description": "Long nested topics", "input": { "text": "User: Let's discuss travel.\nAssistant: Where?\nUser: Paris.\nAssistant: Ok.\nUser: Also, food in Paris.\nAssistant: Many options.\nUser: Back to travel.\nAssistant: Sure." }, "expected": { "type": "segments", "segments": 3, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
     { "id": "case_024", "description": "Short nested topics", "input": { "text": "User: Party?\nAssistant: Yes.\nUser: Food?\nAssistant: Pizza." }, "expected": { "type": "segments", "segments": 2, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_025", "description": "Edge: repeated topic", "input": { "text": "User: Email issue.\nAssistant: Try resetting.\nUser: Email again.\nAssistant: Try support." }, "expected": { "type": "segments", "segments": 2, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" }
  ,
  { "id": "case_026", "description": "Ambiguous: topic switch mid-sentence", "input": { "text": "User: I want to book a flight, but also, can you help with my taxes?" }, "expected": { "type": "segments", "segments": 2, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_027", "description": "Multi-user: three users, three topics", "input": { "text": "User1: Weather?\nUser2: Sports?\nUser3: News?\nAssistant: Weather is sunny.\nAssistant: Sports: Lakers won.\nAssistant: News: Elections soon." }, "expected": { "type": "segments", "segments": 3, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_028", "description": "Long: 10 topic switches", "input": { "text": "User: Topic1\nAssistant: Ok\nUser: Topic2\nAssistant: Ok\nUser: Topic3\nAssistant: Ok\nUser: Topic4\nAssistant: Ok\nUser: Topic5\nAssistant: Ok\nUser: Topic6\nAssistant: Ok\nUser: Topic7\nAssistant: Ok\nUser: Topic8\nAssistant: Ok\nUser: Topic9\nAssistant: Ok\nUser: Topic10\nAssistant: Ok" }, "expected": { "type": "segments", "segments": 10, "tolerance": 2 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_029", "description": "Short: 2 topic switches, 1 ambiguous", "input": { "text": "User: TopicA\nAssistant: Ok\nUser: TopicB and TopicC?\nAssistant: Sure." }, "expected": { "type": "segments", "segments": 3, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_030", "description": "Edge: only system messages", "input": { "text": "System: Welcome\nSystem: Goodbye" }, "expected": { "type": "segments", "segments": 1, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_031", "description": "Multi-user: user and admin", "input": { "text": "User: I need help\nAdmin: What do you need?\nUser: Password reset\nAdmin: Done" }, "expected": { "type": "segments", "segments": 2, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_032", "description": "Ambiguous: topic switch with no clear marker", "input": { "text": "User: Let's talk about food. Now, about travel." }, "expected": { "type": "segments", "segments": 2, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_033", "description": "Long: 5 topics, mixed speakers", "input": { "text": "User: Topic1\nAssistant: Ok\nUser: Topic2\nAssistant: Ok\nUser: Topic3\nAssistant: Ok\nUser: Topic4\nAssistant: Ok\nUser: Topic5\nAssistant: Ok" }, "expected": { "type": "segments", "segments": 5, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_034", "description": "Short: 2 topics, 2 users", "input": { "text": "User1: TopicA\nUser2: TopicB\nAssistant: Ok" }, "expected": { "type": "segments", "segments": 2, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_035", "description": "Edge: only one message", "input": { "text": "User: Hello" }, "expected": { "type": "segments", "segments": 1, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_036", "description": "Ambiguous: topic switch with question", "input": { "text": "User: Can you help with booking? Also, what about my refund?" }, "expected": { "type": "segments", "segments": 2, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_037", "description": "Multi-user: user, admin, assistant", "input": { "text": "User: Issue\nAdmin: Checking\nAssistant: Solution" }, "expected": { "type": "segments", "segments": 3, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_038", "description": "Long: 8 topics, rapid switches", "input": { "text": "User: T1\nAssistant: Ok\nUser: T2\nAssistant: Ok\nUser: T3\nAssistant: Ok\nUser: T4\nAssistant: Ok\nUser: T5\nAssistant: Ok\nUser: T6\nAssistant: Ok\nUser: T7\nAssistant: Ok\nUser: T8\nAssistant: Ok" }, "expected": { "type": "segments", "segments": 8, "tolerance": 2 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_039", "description": "Short: 2 topics, ambiguous switch", "input": { "text": "User: TopicA. Also, TopicB." }, "expected": { "type": "segments", "segments": 2, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_040", "description": "Edge: only assistant, long reply", "input": { "text": "Assistant: Here is a long answer about many things." }, "expected": { "type": "segments", "segments": 1, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_041", "description": "Multi-user: user, system, admin", "input": { "text": "User: Start\nSystem: Info\nAdmin: End" }, "expected": { "type": "segments", "segments": 3, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_042", "description": "Ambiguous: topic switch with context", "input": { "text": "User: Let's talk about A. Now, B is also important." }, "expected": { "type": "segments", "segments": 2, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_043", "description": "Long: 6 topics, mixed users", "input": { "text": "User1: T1\nUser2: T2\nUser1: T3\nUser2: T4\nUser1: T5\nUser2: T6" }, "expected": { "type": "segments", "segments": 6, "tolerance": 2 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_044", "description": "Short: 2 topics, system and user", "input": { "text": "System: Info\nUser: Question" }, "expected": { "type": "segments", "segments": 2, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_045", "description": "Edge: only admin", "input": { "text": "Admin: All set" }, "expected": { "type": "segments", "segments": 1, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
  { "id": "case_046", "description": "Ambiguous: topic switch with list", "input": { "text": "User: I need help with A, B, and C." }, "expected": { "type": "segments", "segments": 3, "tolerance": 1 }, "endpoint": "/api/v1/analyze-text" },
