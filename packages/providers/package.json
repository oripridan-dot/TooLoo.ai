{
  "name": "@tooloo/providers",
  "version": "2.0.0-alpha.0",
  "description": "Unified LLM provider interface with circuit breakers and streaming",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "import": "./dist/adapters/index.js"
    },
    "./circuit-breaker": {
      "types": "./dist/circuit-breaker.d.ts",
      "import": "./dist/circuit-breaker.js"
    },
    "./streaming": {
      "types": "./dist/streaming.d.ts",
      "import": "./dist/streaming.js"
    }
  },
  "scripts": {
    "build": "tsc --noEmit false",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.0",
    "@google/genai": "^0.5.0",
    "@tooloo/core": "workspace:*",
    "openai": "^4.57.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "typescript": "^5.7.2",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "tooloo",
    "ai",
    "llm",
    "providers",
    "openai",
    "anthropic"
  ],
  "author": "Ori Pridan",
  "license": "MIT"
}
