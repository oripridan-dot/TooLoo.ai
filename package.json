    "speak:selection": "node scripts/speak-selection.js",
    "voice:settings": "node scripts/voice-picker.js",
    "benchmark": "node scripts/tooloo.js benchmark",
    "auto-teach": "node scripts/auto-teach.js",
    "generate:fixtures": "node scripts/generate-transcript-fixtures.js",
    "test": "vitest run tests/unit/event-bus.test.js tests/unit/event-schema.test.js tests/unit/training-engine.test.js tests/unit/challenge-engine.test.js tests/unit/provider-selector.test.js tests/unit/budget-manager.test.js tests/integration-service.test.js tests/context-service.test.js tests/analytics-service.test.js tests/orchestration-service.test.js tests/e2e-workflows.test.js tests/integration/web-gateway.test.js tests/integration/provider-service.test.js --config vitest.config.js",
    "test:phase1": "vitest run tests/unit/event-bus.test.js tests/unit/event-schema.test.js tests/integration/web-gateway.test.js --config vitest.config.js",
    "test:phase2": "vitest run tests/unit/training-engine.test.js tests/unit/challenge-engine.test.js tests/unit/provider-selector.test.js tests/unit/budget-manager.test.js tests/integration/provider-service.test.js --config vitest.config.js",
    "test:phase3": "vitest run tests/integration-service.test.js tests/context-service.test.js --config vitest.config.js",
    "test:phase4a": "vitest run tests/analytics-service.test.js --config vitest.config.js",
    "test:phase4b": "vitest run tests/orchestration-service.test.js --config vitest.config.js",
    "test:e2e": "vitest run tests/e2e-workflows.test.js --config vitest.config.js",
    "test:fresh": "vitest run tests/system tests/integration tests/unit --config vitest.config.js",
    "test:all": "npm run test",
    "test:core": "vitest run tests/unit/system.test.js",
    "test:unit": "vitest run --config vitest.config.js --dir tests/unit || true",
    "test:integration": "vitest run --config vitest.config.js --dir tests/api",
    "test:watch": "vitest --config vitest.config.js",
    "test:parser": "node datasets/benchmarks/scripts/parse-harness.js",
    "test:github": "node tests/integration/core-services.integration.test.js",
    "qa:suite": "node scripts/qa-suite.js --report --coverage --verbose",
    "qa:report": "node scripts/qa-suite.js --report && cat qa-coverage-report.json | jq .",
    "lint": "eslint servers/ engine/ scripts/ tests/ --ext .js",
    "lint:fix": "eslint servers/ engine/ scripts/ tests/ --ext .js --fix",
    "format": "prettier --write \"servers/**/*.js\" \"engine/**/*.js\" \"scripts/**/*.js\" \"tests/**/*.js\"",
    "format:check": "prettier --check \"servers/**/*.js\" \"engine/**/*.js\" \"scripts/**/*.js\" \"tests/**/*.js\"",
    "qa:gaps": "node scripts/qa-suite.js --report --coverage | grep -A 20 'Gaps to Fill'",
    "qa:health": "node scripts/qa-suite.js --coverage",
    "qa:web": "node tests/integration/web-server.integration.test.js",
    "qa:training": "node tests/integration/training-server.integration.test.js",
    "qa:budget": "node tests/integration/budget-server.integration.test.js",
    "qa:meta": "node tests/integration/meta-server.integration.test.js",
    "qa:coach": "node tests/integration/coach-server.integration.test.js",
    "qa:e2e": "node tests/e2e/complete-workflows.test.js",
    "qa:product": "node tests/integration/product-dev-server.integration.test.js",
    "qa:capabilities": "node tests/integration/capabilities-server.integration.test.js",
    "qa:reports": "node tests/integration/reports-server.integration.test.js",
    "test:performance": "node tests/performance/baselines.test.js",
    "test:security": "node tests/security/injection-and-auth.test.js",
    "test:smoke": "node tests/smoke-test.js",
    "test:phase4-integration": "node tests/integration-tests-phase4.js",
    "test:phase4-benchmarks": "node tests/performance-benchmarks-phase4.js",
    "test:phase4-chaos": "node tests/chaos-tests-phase4.js",
    "test:phase4-all": "npm run test:phase4-integration && npm run test:phase4-benchmarks && npm run test:phase4-chaos",
    "test:phase6": "node tests/integration-tests-phase6.js",
    "test:phase6-cache": "node tests/integration-tests-phase6.js --cache-only",
    "test:phase6-pool": "node tests/integration-tests-phase6.js --pool-only",
    "test:phase6-optimizer": "node tests/integration-tests-phase6.js --optimizer-only",
    "test:phase6-limiter": "node tests/integration-tests-phase6.js --limiter-only",
    "test:phase6-tracer": "node tests/integration-tests-phase6.js --tracer-only",
    "test:phase6-performance": "node scripts/performance-test-phase6.js",
    "test:phase6-verify": "node scripts/verify-phase6-services.js",
    "test:phase6-full": "npm-run-all test:phase6 test:phase6-performance test:phase6-verify",
