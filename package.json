{
  "name": "tooloo-ai",
  "version": "2.1.361",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "concurrently \"npm run start:synapsys\" \"npm run dev:frontend\"",
    "start:synapsys": "tsx src/main.ts",
    "dev": "concurrently \"tsx watch src/main.ts\" \"npm run dev:frontend\"",
    "dev:frontend": "cd src/web-app && vite",
    "stop:all": "bash scripts/stop-all-services.sh",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.ts && echo 'âœ¨ Linting passed!'",
    "format": "prettier --write \"src/**/*.{ts,json,md}\"",
    "clean": "rm -rf dist temp logs",
    "clean:deep": "npx tsx scripts/clean-workspace.ts",
    "info": "tsx src/cli/index.ts info",
    "update-context": "tsx scripts/update-copilot-context.ts",
    "tooloo": "tsx src/cli/index.ts",
    "tooloo:introspect": "tsx src/cli/index.ts system introspect",
    "tooloo:awareness": "tsx src/cli/index.ts system awareness",
    "tooloo:github:status": "tsx src/cli/index.ts github status",
    "tooloo:github:info": "tsx src/cli/index.ts github info",
    "tooloo:github:context": "tsx src/cli/index.ts github context",
    "branch:status": "git status"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.69.0",
    "@google/genai": "^1.30.0",
    "@tailwindcss/vite": "^4.1.17",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/js-yaml": "^4.0.9",
    "@types/multer": "^2.0.0",
    "@types/sharp": "^0.31.1",
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "chokidar": "^4.0.3",
    "commander": "^11.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "form-data": "^4.0.4",
    "fs-extra": "^11.3.2",
    "glob": "^13.0.0",
    "helmet": "^7.1.0",
    "js-yaml": "^4.1.1",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "openai": "^6.9.1",
    "playwright": "^1.56.0",
    "sharp": "^0.34.5",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.8.1",
    "sqlite3": "^5.1.7",
    "tailwindcss": "^4.1.17",
    "uuid": "^13.0.0",
    "ws": "^8.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/node-fetch": "^2.6.13",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "eslint": "^8.45.0",
    "http-server": "^14.1.1",
    "jsdom": "^27.2.0",
    "nodemon": "^3.1.11",
    "npm-run-all": "^4.1.5",
    "pdfkit": "^0.15.0",
    "prettier": "^3.0.0",
    "supertest": "^7.1.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
