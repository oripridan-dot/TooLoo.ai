{
  "name": "tooloo-synapsys-monorepo",
  "version": "2.0.0-alpha.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.0.0",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "clean": "rm -rf node_modules packages/*/node_modules apps/*/node_modules .turbo",
    "db:push": "turbo run db:push",
    "studio": "turbo run studio"
  },
  "devDependencies": {
    "turbo": "^2.6.3",
    "typescript": "^5.7.2",
    "prettier": "^3.0.0",
    "eslint": "^8.45.0"
  }
}
