    "automate:logs": "node scripts/performance-log-rotator.js",
    "automate:cleanup": "node scripts/code-cleanup.js",
    "automate:docs": "node scripts/update-documentation.js",
    "automate:health": "node scripts/repository-health-check.js",
    "automate:all": "npm-run-all automate:health automate:updates automate:cleanup automate:docs automate:logs",
    "test:adapters": "node scripts/test-adapters.js",
    "test:phase7": "node scripts/test-adapters.js --phase7-only",
    "test:phase11": "node scripts/test-adapters.js --phase11-only",
    "test:endpoints": "node scripts/test-adapters.js --endpoints",
    "test:all:comprehensive": "npm-run-all test:adapters test:smoke qa:suite",
    "checkpoint:4": "node scripts/checkpoint-monitor.js 4",
    "checkpoint:5": "node scripts/checkpoint-monitor.js 5",
    "checkpoint:6": "node scripts/checkpoint-monitor.js 6",
    "checkpoint:7": "node scripts/checkpoint-monitor.js 7",
    "checkpoint:schedule": "node scripts/checkpoint-monitor.js --schedule",
    "checkpoint:monitor": "bash scripts/checkpoint-monitor.sh",
    "checkpoint:runner": "node scripts/checkpoint-runner.js --watch",
    "checkpoint:scheduler": "node scripts/checkpoint-scheduler.js",
    "test:self-capabilities": "node scripts/test-self-capabilities.js",
    "dev:hot-stop": "node scripts/persistent-process-manager.js stop",
    "dev:hot-status": "node scripts/persistent-process-manager.js status",
    "dev:hot-restart": "node scripts/persistent-process-manager.js restart",
    "start:metrics-hub": "tsx src/servers/metrics-hub.ts",
    "start:alert-engine": "tsx src/servers/alert-engine.ts",
    "orchestrator:init": "curl -X POST http://127.0.0.1:3000/api/v1/orchestrator/initialize -H 'Content-Type: application/json'",
    "orchestrator:status": "curl http://127.0.0.1:3000/api/v1/orchestrator/status | jq '.'",
    "orchestrator:map": "curl http://127.0.0.1:3000/api/v1/orchestrator/capability-map | jq '.data'",
    "orchestrator:enable": "curl -X POST http://127.0.0.1:3000/api/v1/orchestrator/enable-autonomous -H 'Content-Type: application/json' -d '{\"enabled\":true,\"mode\":\"safe\",\"maxPerCycle\":2}'",
    "orchestrator:cycle": "curl -X POST http://127.0.0.1:3000/api/v1/orchestrator/activate/cycle -H 'Content-Type: application/json'",
    "orchestrator:test": "bash scripts/test-capability-orchestrator.sh",
    "formatter:view": "open http://127.0.0.1:3000/web-app/response-formatter-enhanced.html || xdg-open http://127.0.0.1:3000/web-app/response-formatter-enhanced.html",
    "tooloo:executor": "node scripts/tooloo-executor.js",
    "tooloo:auto-execute": "node scripts/tooloo-executor.js --save-results",
    "tooloo:auto-execute:seq": "node scripts/tooloo-executor.js --sequential --save-results",
    "tooloo:auto-execute:file": "node scripts/tooloo-executor.js .tooloo-suggestions.json --save-results",
    "tooloo:auto-execute:watch": "nodemon --watch .tooloo-suggestions.json --exec 'npm run tooloo:auto-execute'",
    "tooloo:test-capability": "node scripts/test-self-capabilities.js",
    "tooloo:introspect": "curl http://127.0.0.1:3000/api/v1/system/introspect | jq '.'",
    "tooloo:awareness": "curl http://127.0.0.1:3000/api/v1/system/awareness | jq '.'",
    "tooloo:github:status": "curl http://127.0.0.1:3000/api/v1/github/health | jq '.'",
    "tooloo:github:info": "curl http://127.0.0.1:3000/api/v1/github/info | jq '.'",
    "tooloo:github:context": "curl http://127.0.0.1:3000/api/v1/github/context | jq '.context' | head -50",
    "servers:start": "node scripts/server-manager.js start",
    "servers:stop": "node scripts/server-manager.js stop",
    "servers:restart": "node scripts/server-manager.js restart",
    "servers:status": "node scripts/server-manager.js status",
    "servers:start:web": "node scripts/server-manager.js start web-server",
    "servers:start:optimization": "node scripts/server-manager.js start optimization-server",
    "servers:start:acquisition": "node scripts/server-manager.js start acquisition-server",
    "servers:dev": "NODE_ENV=development npm run servers:start",
    "servers:test": "NODE_ENV=testing npm run servers:start",
    "servers:prod": "NODE_ENV=production npm run servers:start",
    "servers:hot-reload": "NODE_ENV=development node scripts/server-manager.js start --hot-reload",
    "servers:monitor": "node scripts/server-manager.js status && watch -n 2 'npm run servers:status'",
    "start:ts": "tsx src/index.ts"
