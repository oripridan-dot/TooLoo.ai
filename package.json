{
  "name": "tooloo-ai",
  "version": "2.1.20",
  "description": "TooLoo.ai multi-service personal AI development platform",
  "type": "module",
  "private": true,
  "scripts": {
    "version:tag": "node scripts/version-tagger.cjs",
    "info": "echo 'ðŸ“‹ TooLoo.ai multi-service workspace'",
    "build": "echo 'âœ… Static HTML app - no build needed. Run: npm run start'",
    "dev": "bash scripts/startup.sh",
    "dev:hot": "bash scripts/hot-reload-dev.sh",
    "start": "bash scripts/startup.sh",
    "start:guaranteed": "bash scripts/startup.sh",
    "start:synapsys": "tsx src/main.ts",
    "build:synapsys": "tsc",
    "start:web": "tsx src/nexus/interface/web-server.ts",
    "start:optimization": "tsx src/legacy_migration/servers/optimization-server.ts",
    "start:acquisition": "tsx src/legacy_migration/servers/acquisition-server.ts",
    "start:budget": "tsx src/precog/oracle/budget-server.ts",
    "start:guide": "tsx src/legacy_migration/servers/guide-server.ts",
    "start:product": "tsx src/cortex/planning/product-development-server.ts",
    "start:segmentation": "tsx src/cortex/memory/segmentation-server.ts",
    "start:reports": "tsx src/nexus/interface/reports-server.ts",
    "start:capabilities": "tsx src/cortex/system-model/capabilities-server.ts",
    "start:simple": "tsx src/legacy_migration/simple-api-server.ts",
