    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-query": "^5.90.11",
    "@types/better-sqlite3": "^7.6.13",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/js-yaml": "^4.0.9",
    "@types/multer": "^2.0.0",
    "@types/sharp": "^0.31.1",
    "axios": "^1.13.2",
    "better-sqlite3": "^12.4.6",
    "cheerio": "^1.1.2",
    "chokidar": "^4.0.3",
    "commander": "^11.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "form-data": "^4.0.4",
    "framer-motion": "^12.23.24",
    "fs-extra": "^11.3.2",
    "glob": "^13.0.0",
    "helmet": "^7.1.0",
