{
  "name": "tooloo-synapsys",
  "version": "3.3.399",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prestart": "node scripts/check-working-directory.cjs",
    "predev": "node scripts/check-working-directory.cjs",
    "predev:all": "node scripts/check-working-directory.cjs",
    "pretest": "node scripts/check-working-directory.cjs",
    "prebuild:frontend": "node scripts/check-working-directory.cjs",
    "start": "concurrently \"npm run start:synapsys\" \"npm run dev:frontend\"",
    "start:synapsys": "tsx src/main.ts",
    "start:rnd": "bash scripts/dev-rnd.sh",
    "dev:all": "bash scripts/start-clean.sh",
    "dev:ensure": "bash scripts/ensure-running.sh",
    "dev": "bash scripts/dev.sh",
    "dev:rnd": "bash scripts/dev-rnd.sh",
    "dev:frontend": "vite --config src/web-app/vite.config.ts",
    "build:frontend": "vite build --config src/web-app/vite.config.ts",
    "preview:frontend": "vite preview --config src/web-app/vite.config.ts",
    "stop": "bash scripts/stop.sh",
    "stop:all": "bash scripts/stop.sh",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:fuzz": "npx tsx scripts/run-fuzzer.ts",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --ignore-pattern \"src/web-app/**\" && echo 'âœ¨ Linting passed!'",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "storybook": "cd src/web-app && npm run storybook",
    "build-storybook": "cd src/web-app && npm run build-storybook",
    "generate:arch": "tsx scripts/generate-arch-graph.ts",
    "clean": "rm -rf dist temp logs",
    "clean:deep": "npx tsx scripts/clean-workspace.ts",
    "info": "tsx src/cli/index.ts info",
    "update-context": "tsx scripts/update-copilot-context.ts",
    "tooloo": "tsx src/cli/index.ts",
    "tooloo:introspect": "tsx src/cli/index.ts system introspect",
    "tooloo:awareness": "tsx src/cli/index.ts system awareness",
    "tooloo:github:status": "tsx src/cli/index.ts github status",
    "tooloo:github:info": "tsx src/cli/index.ts github info",
    "tooloo:github:context": "tsx src/cli/index.ts github context",
    "branch:status": "git status",
    "check:duplicates": "tsx scripts/check-duplicates.ts",
    "check:duplicates:all": "tsx scripts/check-duplicates.ts --all",
    "analyze:architecture": "tsx src/core/architecture-analyzer.ts",
    "analyze:components": "tsx src/core/component-registry.ts",
    "generate:responsibilities": "tsx -e \"import { registry } from './src/core/module-registry.js'; registry.saveResponsibilitiesDoc()\"",
    "monitor:learning": "node scripts/monitor-learning.js",
    "qa:check": "tsx -e \"import { wireVerifier } from './src/qa/wiring/wire-verifier.js'; wireVerifier.verify().then(r => console.log(JSON.stringify(r, null, 2)))\"",
    "qa:fix": "tsx -e \"import { autonomousFixer } from './src/qa/agent/autonomous-fixer.js'; autonomousFixer.processQueue(5).then(r => console.log(JSON.stringify(r, null, 2)))\"",
    "qa:metrics": "tsx -e \"import { qaGuardianAgent } from './src/qa/agent/qa-guardian-agent.js'; console.log(JSON.stringify(qaGuardianAgent.getMetrics(), null, 2))\"",
    "qa:schema": "tsx -e \"import { apiSchemaGenerator } from './src/qa/schema/api-schema-generator.js'; apiSchemaGenerator.generateAndSave().then(p => console.log('Saved to:', p))\"",
    "qa:breaking": "tsx -e \"import { apiSchemaGenerator } from './src/qa/schema/api-schema-generator.js'; apiSchemaGenerator.detectBreakingChanges('docs/openapi.json').then(r => console.log(JSON.stringify(r, null, 2)))\"",
    "qa:validate": "tsx -e \"import { runValidation } from './src/qa/validation/feature-validator.js'; runValidation('changed');\"",
    "qa:validate:full": "tsx -e \"import { runValidation } from './src/qa/validation/feature-validator.js'; runValidation('full');\"",
    "qa:validate:file": "tsx -e \"import { runValidation } from './src/qa/validation/feature-validator.js'; runValidation('file', process.argv[2]);\"",
    "precommit": "tsx scripts/pre-commit-check.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.69.0",
    "@google/genai": "^1.30.0",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-query": "^5.90.11",
    "@types/better-sqlite3": "^7.6.13",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/js-yaml": "^4.0.9",
    "@types/multer": "^2.0.0",
    "@types/sharp": "^0.31.1",
    "axios": "^1.13.2",
    "better-sqlite3": "^12.4.6",
    "chalk": "^5.6.2",
    "chart.js": "^4.5.1",
    "cheerio": "^1.1.2",
    "chokidar": "^4.0.3",
    "commander": "^11.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "eventsource": "^4.1.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "form-data": "^4.0.4",
    "framer-motion": "^12.23.24",
    "fs-extra": "^11.3.2",
    "glob": "^13.0.0",
    "helmet": "^7.1.0",
    "js-yaml": "^4.1.1",
    "katex": "^0.16.27",
    "lucide-react": "^0.294.0",
    "mermaid": "^10.9.5",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "openai": "^6.9.1",
    "playwright": "^1.56.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-live": "^4.1.8",
    "react-markdown": "^9.0.1",
    "react-resizable-panels": "^3.0.6",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.34.5",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.8.1",
    "sqlite3": "^5.1.7",
    "tailwindcss": "^4.1.17",
    "uuid": "^13.0.0",
    "ws": "^8.18.3",
    "yaml": "^2.8.2",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.2.1",
    "dependency-cruiser": "^17.3.2",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jsdom": "^27.2.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.0",
    "supertest": "^7.1.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^5.0.0",
    "vitest": "^3.2.4"
  }
}
