{
  "name": "tooloo-ai",
  "version": "2.1.48",
  "description": "TooLoo.ai multi-service personal AI development platform",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "tsx src/main.ts",
    "start:synapsys": "npm start",
    "dev": "tsx watch src/main.ts",
    "test": "vitest run",
    "lint": "eslint src/ --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rm -rf dist node_modules",
    "info": "echo 'ðŸ“‹ TooLoo.ai multi-service workspace'",
    "tooloo:introspect": "curl http://127.0.0.1:4000/api/v1/system/introspect | jq '.'",
    "tooloo:awareness": "curl http://127.0.0.1:4000/api/v1/system/awareness | jq '.'",
    "tooloo:github:status": "curl http://127.0.0.1:4000/api/v1/github/health | jq '.'",
    "tooloo:github:info": "curl http://127.0.0.1:4000/api/v1/github/info | jq '.'",
    "tooloo:github:context": "curl http://127.0.0.1:4000/api/v1/github/context | jq '.context' | head -50"
  },
  "keywords": [
    "ai",
    "multi-service",
    "personal-assistant",
    "segmentation",
    "orchestration"
  ],
  "author": "TooLoo.ai Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.69.0",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "axios": "^1.13.2",
    "chokidar": "^4.0.3",
    "commander": "^11.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "form-data": "^4.0.4",
    "fs-extra": "^11.3.2",
    "glob": "^13.0.0",
    "helmet": "^7.1.0",
    "node-fetch": "^3.3.2",
    "playwright": "^1.56.0",
    "socket.io": "^4.7.4",
    "sqlite3": "^5.1.7",
    "uuid": "^13.0.0",
    "ws": "^8.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/node-fetch": "^2.6.13",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^8.45.0",
    "http-server": "^14.1.1",
    "nodemon": "^3.1.11",
    "npm-run-all": "^4.1.5",
    "pdfkit": "^0.15.0",
    "prettier": "^3.0.0",
    "supertest": "^7.1.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
