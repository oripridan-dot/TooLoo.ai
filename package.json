    "lucide-react": "^0.294.0",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "openai": "^6.9.1",
    "playwright": "^1.56.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "sharp": "^0.34.5",
    "socket.io": "^4.7.4",
